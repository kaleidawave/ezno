{
	"name": "unplugin-ezno",
	"version": "0.0.20",
	"description": "Ezno as a plugin",
	"main": "./dist/index.cjs",
	"module": "./dist/index.mjs",
	"types": "./types.d.ts",
	"license": "MIT",
	"exports": {
		".": {
			"require": "./dist/index.cjs",
			"import": "./dist/index.mjs",
			"types": "./types.d.ts"
		}
	},
	"dependencies": {
		"ezno": "^0.0.20",
		"unplugin": "^1.3.1"
	},
	"keywords": [
		"type-checker",
		"vite",
		"webpack",
		"ezno"
	],
	"files": [
		"dist"
	],
	"author": {
		"name": "Ben",
		"email": "kaleidawave@gmail.com",
		"url": "https://kaleidawave.github.io/"
	},
	"funding": {
		"type": "individual",
		"url": "https://github.com/sponsors/kaleidawave"
	},
	"devDependencies": {
		"unbuild": "^1.1.2"
	},
	"scripts": {
		"build": "unbuild",
		"build-js": "unbuild"
	},
	"build": {
		"entries": [
			{
				"builder": "rollup",
				"input": "./index"
			}
		],
		"rollup": {
			"emitCJS": true,
			"commonjs": true
		}
	}
}
